{
  "name": "intensivesquad",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "account": "pnpm workspace @intensivesquad/foundry account",
    "chain": "pnpm workspace @intensivesquad/foundry chain",
    "compile": "pnpm workspace @intensivesquad/foundry compile",
    "deploy": "pnpm workspace @intensivesquad/foundry deploy",
    "deploy:verify": "pnpm workspace @intensivesquad/foundry deploy:verify",
    "fork": "pnpm workspace @intensivesquad/foundry fork",
    "foundry:lint": "pnpm workspace @intensivesquad/foundry lint",
    "foundry:test": "pnpm workspace @intensivesquad/foundry test",
    "verify": "pnpm workspace @intensivesquad/foundry verify",
    "generate": "pnpm workspace @intensivesquad/foundry generate",
    "postinstall": "husky install",
    "next:check-types": "pnpm workspace @intensivesquad/webapps check-types",
    "next:format": "pnpm workspace @intensivesquad/webapps format",
    "next:lint": "pnpm workspace @intensivesquad/webapps lint",
    "__precommit": "lint-staged",
    "__prepare": "install-self-peers -- --ignore-scripts",
    "start": "pnpm workspace @intensivesquad/webapps dev",
    "test": "pnpm foundry:test",
    "vercel": "pnpm workspace @intensivesquad/webapps vercel",
    "vercel:yolo": "pnpm workspace @intensivesquad/webapps vercel:yolo"
  }
}
